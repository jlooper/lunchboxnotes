<%= render "shared/error_messages", :target => @user_session %>
    
<%= form_for(@user_session) do |f| %>
<div class="field">
  <%= f.label :email %><br />
  <%= f.text_field :email %><br />
</div>
<div class="field">
  <%= f.label :password %><br />
  <%= f.password_field :password %><br />
</div>
<div class="field">
  <%= f.check_box :remember_me %><%= f.label :remember_me %><br />
</div>
  <%= f.submit "Login" %>
<% end %>
